<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="398.0" prefWidth="484.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.LoginController">
    <PasswordField fx:id="PasswordTextField" layoutX="134.0" layoutY="217.0" prefHeight="25.0" prefWidth="217.0" promptText="Password">
      <font>
         <Font name="Rockwell" size="14.0" />
      </font></PasswordField>
    <TextField fx:id="UsernameTextField" layoutX="134.0" layoutY="185.0" prefHeight="25.0" prefWidth="217.0" promptText="Username">
      <font>
         <Font name="Rockwell" size="14.0" />
      </font></TextField>
    <Text fx:id="Title" layoutX="118.0" layoutY="83.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Appointment Manager" textAlignment="CENTER" wrappingWidth="249.0">
        <font>
            <Font name="Rockwell Bold" size="30.0" />
        </font>
    </Text>
    <Button fx:id="LoginButton" layoutX="149.0" layoutY="285.0" mnemonicParsing="false" onAction="#loginToApplication" text="Login">
      <font>
         <Font name="Rockwell Bold" size="13.0" />
      </font></Button>
    <Button fx:id="ExitButton" layoutX="285.0" layoutY="285.0" mnemonicParsing="false" onAction="#exit" prefHeight="25.0" prefWidth="46.0" text="Exit">
      <font>
         <Font name="Rockwell Bold" size="13.0" />
      </font></Button>
    <Label fx:id="LocationLabel" layoutX="204.0" layoutY="254.0" text="Location">
      <font>
         <Font name="Rockwell Italic" size="12.0" />
      </font></Label>
</AnchorPane>
